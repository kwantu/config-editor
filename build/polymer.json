{
  "entrypoint": "index.html",
  "shell": "src/app/main.html",
  "fragments": [
    "src/elements/**/*"
  ],
  "sources": [
    "bower.json",
    "index.html",
    "src/app/main.html",
    "src/elements/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.min.js",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "bundle": true,
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ],
    "ignoreWarnings": []
  }
}